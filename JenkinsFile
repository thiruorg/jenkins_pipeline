node {
      

   stage('cloning') {
    // some block
     git 'https://github.com/thirumoorthir/rundeck-puppetenterprise-nodes-plugin.git'
}

   stage ('building code'){
       sh './gradlew build'
   }

   stage ('archive'){

     archiveArtifacts artifacts: '/build/libs/*', fingerprint: true, onlyIfSuccessful: true
   
   }



}
